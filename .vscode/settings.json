{
    "python.analysis.typeCheckingMode": "basic",
    "editor.defaultFormatter": "ms-python.black-formatter",
    "black-formatter.interpreter": [
        "/opt/conda/bin/python"
    ],
    "black-formatter.args": [
        "--line-length=3000"
    ],
    "cSpell.words": [
        "autoanchor",
        "colorstr",
        "imgsz",
        "imread",
        "matplotlib",
        "mloss",
        "noautoanchor",
        "optim",
        "pbar",
        "pyplot",
        "torchinfo",
        "torchvision",
        "tqdm",
        "Trainloader",
        "wandb",
        "yolo",
        "yolov7"
    ]
}